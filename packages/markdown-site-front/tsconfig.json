{
  "compilerOptions": {
    "target": "ES2015",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "alwaysStrict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "noEmitHelpers": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "sourceRoot": ".",
    "baseUrl": ".",
    "jsx": "react",
    "lib": ["dom", "esnext", "ES2015"]
  },
  "exclude": ["dist", ".awcache", "node_modules"],
  "include": ["src"]
}
